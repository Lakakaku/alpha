{
  "version": 2,
  "name": "vocilia-customer-prod",
  "builds": [
    {
      "src": "apps/customer/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": "apps/customer/.next"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/customer/$1"
    }
  ]
}
