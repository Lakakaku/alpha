# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ACCESS_TOKEN=your-access-token
SUPABASE_PROJECT_REF=your-project-ref

# Next.js Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Railway Configuration (for backend deployment)
RAILWAY_TOKEN=your-railway-token

# QR Verification System Configuration
API_BASE_URL=http://localhost:3001
QR_SESSION_EXPIRY_MINUTES=30
FRAUD_DETECTION_RATE_LIMIT=10
FRAUD_DETECTION_WINDOW_MINUTES=60
VERIFICATION_TIME_TOLERANCE_MINUTES=2
VERIFICATION_AMOUNT_TOLERANCE_SEK=2

# Security Configuration
SESSION_TOKEN_LENGTH=64
RATE_LIMIT_WINDOW_MS=3600000
RATE_LIMIT_MAX_REQUESTS=10

# AI Call System Configuration
# 46elks Telephony Provider (Primary)
FORTYSIXELKS_API_USERNAME=your-46elks-username
FORTYSIXELKS_API_PASSWORD=your-46elks-password
FORTYSIXELKS_WEBHOOK_URL=https://your-domain.com/api/webhooks/telephony

# Twilio Telephony Provider (Fallback)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number
TWILIO_WEBHOOK_URL=https://your-domain.com/api/webhooks/telephony

# OpenAI Configuration for AI Calls
OPENAI_API_KEY=your-openai-api-key
OPENAI_ORGANIZATION_ID=your-openai-org-id
OPENAI_REALTIME_MODEL=gpt-4o-mini
OPENAI_VOICE_MODEL=sv-SE-MattiasNeural
OPENAI_VOICE_NAME=alloy

# Call System Configuration
CALL_DURATION_MAX_SECONDS=120
CALL_DURATION_WARNING_THRESHOLD=80
CALL_INITIATION_TIMEOUT_SECONDS=300
CALL_MAX_COST_USD=0.25
CALL_DEFAULT_PROVIDER=46elks

# Question Selection Configuration
QUESTION_SELECTION_ALGORITHM=frequency_weighted
QUESTION_MAX_RESPONSE_TIME_SECONDS=30
QUESTION_MIN_FREQUENCY=1
QUESTION_MAX_FREQUENCY=100

# Call Quality and Monitoring
CALL_QUALITY_MIN_SCORE=0.7
CALL_RECORDING_ENABLED=true
CALL_TRANSCRIPT_ENABLED=true
CALL_ANALYTICS_ENABLED=true

# Redis Configuration for Session Management
REDIS_URL=redis://localhost:6379
REDIS_SESSION_TTL=7776000  # 90 days in seconds
REDIS_CONNECTION_TIMEOUT=5000
REDIS_RETRY_ATTEMPTS=3

# AI Analysis Configuration
AI_CALLS_ENABLED=true
AI_ANALYSIS_ENABLED=true
BUSINESS_ANALYSIS_ENABLED=true
AI_QUALITY_SCORE_MIN=0.02
AI_QUALITY_SCORE_MAX=0.15
AI_FRAUD_DETECTION_THRESHOLD=0.7

# Communication Systems Configuration
# Twilio SMS Provider Configuration
TWILIO_SMS_ACCOUNT_SID=your-twilio-sms-account-sid
TWILIO_SMS_AUTH_TOKEN=your-twilio-sms-auth-token
TWILIO_SMS_PHONE_NUMBER=your-twilio-sms-phone-number
TWILIO_SMS_WEBHOOK_URL=https://your-domain.com/api/webhooks/twilio-delivery-status

# SMS Provider Configuration
SMS_PROVIDER=twilio
SMS_DEFAULT_LANGUAGE=sv
SMS_RETRY_ATTEMPTS=3
SMS_RETRY_INTERVALS=0,300,1800  # immediate, 5min, 30min (in seconds)
SMS_MAX_CHARACTER_LIMIT=1600
SMS_RATE_LIMIT_PER_MINUTE=30

# Communication Templates Configuration
TEMPLATE_DEFAULT_LANGUAGE=sv
TEMPLATE_SUPPORTED_LANGUAGES=sv,en
TEMPLATE_VALIDATION_ENABLED=true
TEMPLATE_VERSION_CONTROL_ENABLED=true

# Support System Configuration
SUPPORT_DEFAULT_SLA_HOURS=2
SUPPORT_PHONE_SLA_SECONDS=30
SUPPORT_EMAIL_SLA_HOURS=2
SUPPORT_CHAT_SLA_HOURS=2
SUPPORT_ESCALATION_ENABLED=true
SUPPORT_AUTO_ASSIGNMENT_ENABLED=true

# Notification Processing Configuration
NOTIFICATION_PROCESSOR_INTERVAL_SECONDS=30
NOTIFICATION_BATCH_SIZE=50
NOTIFICATION_RETRY_ENABLED=true
NOTIFICATION_QUIET_HOURS_START=22:00
NOTIFICATION_QUIET_HOURS_END=08:00

# Communication Logs and Audit Configuration
COMMUNICATION_AUDIT_ENABLED=true
COMMUNICATION_LOG_RETENTION_DAYS=365
COMMUNICATION_METRICS_ENABLED=true
